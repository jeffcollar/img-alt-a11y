{
    "manifest_version": 2,
    "name": "Image Alt Tag A11y",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["content.js"],
            "css": ["style.css"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {},
    "icons": {
        "48": "icon48.png",
        "128": "icon128.png"
    }
  }